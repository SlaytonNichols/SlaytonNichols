{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "Blogifier": {
        "DbProvider": "SqlServer",
        "Avatar": "admin/img/avatar.png",

        "SendGridApiKey": "YOUR-SENDGRID-API-KEY",
        "SendGridEmailFrom": "admin@blog.com",
        "SendGridEmailFromName": "Blog admin"
    },
    "FeatureManagement": {
        "Demo": false,
        "Email": {
            "EnabledFor": [{
                "Name": "EmailFilter"
            }]
        },
        "GenerateThumbs": true
    }
}